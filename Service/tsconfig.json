{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@route/*": [
        "./Routes/*"
      ],
      "@serve/*": [
        "./Services/*"
      ],
      "@model/*": [
        "./Models/*"
      ]
    },
    "preserveSymlinks": false,
    "target": "ESNEXT",
    "outDir": "./dist/",
    "module": "commonjs",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}